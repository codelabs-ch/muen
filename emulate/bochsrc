romimage: file=$BXSHARE/BIOS-bochs-latest
vgaromimage: file=$BXSHARE/VGABIOS-lgpl-latest

cpu: model=corei7_ivy_bridge_3770k
cpu: count=1:2:2, ips=50000000, reset_on_triple_fault=0, ignore_bad_msrs=1, msrs="msrs.def"
cpu: cpuid_limit_winnt=0

memory: guest=5120, host=512

ata0: enabled=1, ioaddr1=0x1f0, ioaddr2=0x3f0, irq=14
ata0-master: type=cdrom, path=muen.iso, status=inserted
boot: cdrom

log: bochsout.txt
panic: action=ask
error: action=report
info: action=report
debug: action=ignore

com1: enabled=1, mode=file, dev=serial.out

debugger_log: debugger.out
magic_break: enabled=1

#pci: enabled=1, chipset=i440fx, slot1=usb_ohci
#usb_ohci: enabled=1, port2=mouse
